{
  "name": "vot",
  "module": "src/index.ts",
  "type": "module",
  "devDependencies": {
    "@eslint/eslintrc": "latest",
    "@eslint/js": "latest",
    "@types/bun": "latest",
    "@types/node-schedule": "latest",
    "prettier": "latest",
    "pino-pretty": "latest"
  },
  "scripts": {
    "fmt": "bun run prettier --write src",
    "check": "tsc --noEmit",
    "bundle": "bun build ./src/index.ts --target bun --outfile vot.js",
    "dev": "bun --bun --watch src/index.ts | pino-pretty",
    "start": "bunx prisma db push --accept-data-loss && NODE_ENV=production  bun --bun src/index.ts | pino-pretty"
  },
  "peerDependencies": {
    "typescript": "latest"
  },
  "dependencies": {
    "@discordjs/voice": "latest",
    "@elysiajs/cors": "latest",
    "@napi-rs/canvas": "latest",
    "@prisma/client": "latest",
    "@types/ioredis": "^5.0.0",
    "axios": "latest",
    "discord.js": "^14.16.2",
    "elysia": "latest",
    "genius-lyrics": "^4.4.7",
    "ioredis": "^5.4.1",
    "kazagumo": "^3.2.1",
    "kazagumo-spotify": "^2.1.0",
    "musicard": "latest",
    "node-schedule": "^2.1.1",
    "pino": "latest",
    "prisma": "latest",
    "prisma-redis-middleware": "latest",
    "puppeteer": "latest",
    "query-string": "latest",
    "sharp": "latest",
    "shoukaku": "latest"
  }
}