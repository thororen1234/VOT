{
  "name": "vot",
  "module": "src/index.ts",
  "type": "module",
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.8.0",
    "@types/bun": "latest",
    "@types/node-schedule": "^2.1.6",
    "prettier": "latest",
    "pino-pretty": "latest"
  },
  "scripts": {
    "fmt": "bun run prettier --write src",
    "check": "tsc --noEmit",
    "bundle": "bun build ./src/index.ts --target bun --outfile vot.js",
    "dev": "bun --bun --watch src/index.ts | pino-pretty",
    "start": "bunx prisma db push && NODE_ENV=production  bun --bun src/index.ts | pino-pretty"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@devraelfreeze/discordjs-pagination": "^2.7.6",
    "@prisma/client": "^5.10.2",
    "@types/ioredis": "^5.0.0",
    "@types/pngjs": "latest",
    "@napi-rs/canvas": "latest",
    "@discordjs/voice": "latest",
    "axios": "latest",
    "crc": "latest",
    "dayjs": "latest",
    "elysia": "latest",
    "gifuct-js": "latest",
    "musicard": "latest",
    "pino": "latest",
    "pngjs": "latest",
    "puppeteer": "latest",
    "query-string": "latest",
    "sharp": "latest",
    "shoukaku": "latest",
    "discord.js": "^14.15.3",
    "ioredis": "^5.4.1",
    "kazagumo": "^3.0.5",
    "kazagumo-spotify": "^2.1.0",
    "node-schedule": "^2.1.1",
    "prisma": "^5.16.1",
    "prisma-redis-middleware": "^4.8.0"
  }
}